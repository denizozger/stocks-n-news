{
  "name": "stocks-n-news",
  "version": "0.0.1",
  "description": "A Node.js app displaying stock prices and news about some companies.",
  "engines": {
    "node": "5.9.1"
  },
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "develop": "node_modules/.bin/nodemon",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "node_modules/.bin/mocha -R list -g 'Integration Tests'",
    "test:unit": "node_modules/.bin/mocha -R list -g 'Unit Tests'"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "ejs": "2.4.1",
    "express": "4.13.3",
    "mongodb": "^2.2.5",
    "request": "^2.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/denizozger/stocks-n-news"
  },
  "devDependencies": {
    "mocha": "^3.0.2",
    "nodemon": "^1.10.0",
    "should": "^10.0.0",
    "supertest": "^2.0.0"
  }
}
